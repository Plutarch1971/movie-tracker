{
    "name": "movie-tracker",
    "version": "1.0.0",
    "description": "This app will be design to keep track of movies for the users.",
    "scripts": {
        "start": "cd server && npm run start",
        "develop": "concurrently \"cd Server && npm run watch\" \"cd Client && npm run dev\"",
        "build": "vite build",
        "dev": "vite",
        "preview": "vite preview",
        "render-build": "npm install && cd Client && npm install && npm run build && mkdir -p ../Server/client && cp -r dist ../Server/client/ && cd ../Server && npm install && npm run build"
        "test": "npx cypress run --browser electron",
        "test-component": "npx cypress run --component",
        "test-gui": "npx cypress open"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/lodash": "^4.17.13",
        "concurrently": "^8.2.0",
        "cypress": "^13.17.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "ts-node": "^10.9.2",
        "vite": "^5.0.0"
    },
    "dependencies": {
        "@types/bcrypt": "^5.0.2",
        "@vitejs/plugin-react": "^4.3.1",
        "bcrypt": "^5.1.1",
        "dotenv": "^16.4.7",
        "graphql-tag": "^2.12.6",
        "vite-plugin-env-compatible": "^2.0.1",
