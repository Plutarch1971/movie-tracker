{
    "name": "movie-tracker",
    "version": "1.0.0",
    "description": "This app will be design to keep track of movies for the users.",
    "scripts": {
        "start": "node server/dist/server.js",
        "seed": "cd server && npm run seed",
        "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm run dev\"",
        "install-server": "cd server && npm install",
        "install-client": "cd client && npm install",
        "postinstall": "npm run install-client",
        "build": "npm run build-client",
        "build-client": "cd client && npm run build",
        "render-build": "npm install && npm run build",
        "test": "npx cypress run --browser electron",
        "test-component": "npx cypress run --component",
        "test-gui": "npx cypress open"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/lodash": "^4.17.13",
        "concurrently": "^8.2.0",
        "cypress": "^13.17.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "ts-node": "^10.9.2"
    },
    "dependencies": {
        "@types/bcrypt": "^5.0.2",
        "@vitejs/plugin-react": "^4.3.1",
        "bcrypt": "^5.1.1",
        "dotenv": "^16.4.7",
        "graphql-tag": "^2.12.6",
        "vite-plugin-env-compatible": "^2.0.1",
        "vitest": "^2.0.3"
    },
    "main": "index.js",
    "engines": {
        "node": ">=18.0.0"
    }
}
